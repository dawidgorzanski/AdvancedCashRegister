<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane onKeyPressed="#handleKeyAction" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cashregister.controllers.LoginWindowController">
    <children>
        <Pane prefHeight="200.0" prefWidth="200.0">
            <children>
                <Button fx:id="enter" layoutX="134.0" layoutY="154.0" mnemonicParsing="false" onAction="#handleCheckLoginButtonAction" text="Zaloguj" />
                <Label layoutX="14.0" layoutY="14.0" text="Użytkownik:" />
                <TextField fx:id="userIdTextField" layoutX="14.0" layoutY="38.0" />
                <Label layoutX="14.0" layoutY="83.0" text="Hasło:" />
                <PasswordField fx:id="userPasswordField" layoutX="14.0" layoutY="113.0" />
                <Button layoutX="21.0" layoutY="154.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="Anuluj" />
            </children>
        </Pane>
    </children>
</AnchorPane>
