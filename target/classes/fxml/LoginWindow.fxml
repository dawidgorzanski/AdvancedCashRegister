<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane onKeyPressed="#handleKeyAction" prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/loginWindow.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cashregister.controllers.LoginWindowController">
    <children>
        <Pane prefHeight="400.0" prefWidth="335.0">
            <children>
                <Button id="enter" fx:id="enter" layoutX="65.0" layoutY="233.0" mnemonicParsing="false" onAction="#handleCheckLoginButtonAction" prefHeight="50.0" prefWidth="200.0" text="ZALOGUJ" />
                <TextField fx:id="userIdTextField" layoutX="65.0" layoutY="69.0" prefHeight="55.0" prefWidth="200.0" promptText="użytkownik" />
                <PasswordField fx:id="userPasswordField" layoutX="65.0" layoutY="153.0" prefHeight="55.0" prefWidth="200.0" promptText="hasło" />
                <Button id="exit" layoutX="85.0" layoutY="320.0" mnemonicParsing="false" onAction="#handleQuitButtonAction" prefHeight="30.0" prefWidth="160.0" text="WYJDŹ" />
            </children>
        </Pane>
    </children>
</AnchorPane>
